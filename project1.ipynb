    "import math\n",
    "\n",
    "# Get the radius from the user\n",
    "radius = float(input(\"Enter the radius of the circle: \"))\n",
    "\n",
    "# Calculate the area of the circle\n",
    "area = math.pi * radius**2\n",
    "\n",
    "# Print the result\n",
    "print(\"The area of the circle with radius\", radius, \"is:\", area)\n"
